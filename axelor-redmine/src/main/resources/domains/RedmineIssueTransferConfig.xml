<?xml version="1.0" encoding="UTF-8"?>
<domain-models
	xmlns="http://axelor.com/xml/ns/domain-models"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

	<module name="redmine" package="com.axelor.apps.redmine.db" />

	<entity name="RedmineIssueTransferConfig" lang="java">

		<string name="source" title="Source" />
		<string name="target" title="Target" />

		<many-to-one name="projectTransfer"
			ref="com.axelor.apps.redmine.db.RedmineIssueTransfer" />
		<many-to-one name="trackerTransfer"
			ref="com.axelor.apps.redmine.db.RedmineIssueTransfer" />
		<many-to-one name="statusTransfer"
			ref="com.axelor.apps.redmine.db.RedmineIssueTransfer" />
		<many-to-one name="priorityTransfer"
			ref="com.axelor.apps.redmine.db.RedmineIssueTransfer" />

	</entity>

</domain-models>
